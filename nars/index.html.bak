  function slideposter(){
		var imgs=$('.slides').find('img');
		var first=imgs.eq(0);
		var second=imgs.eq(1);
		second.fadeIn(400);
		first.fadeOut(400,function(){ 
		  $('.slides').append(first);
		});
    }

	function slideposterback(){
		var imgs=$('.slides').find('img');
		var first=imgs.eq(0);
		var second=imgs.eq(2);
		second.fadeIn(400);
		first.fadeOut(400,function(){   
		  $('.slides').prepend(second);
		});
    }
	
	var timer=setInterval(slideposter, 2000);
	setInterval(timer,2000);
    
	$('.slides,.prev,.next').on('mouseenter',function(){
		clearInterval(timer,2000);
	
	});
	
	$('.slides,.prev,.next').on('mouseleave',function(){
		timer=setInterval(slideposter,2000);

    });
    
	  
	$('.next').on('click',function(event){
		event.preventDefault();
			
		    });
	 $('.next').click(function(){
	    slideposter();
	  });

	  $('.prev').on('click',function(event){
		event.preventDefault();
		  
		});
	  $('.prev').click(function(){
	    slideposterback();
      
	  });

  }); 

