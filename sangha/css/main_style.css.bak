
 body,ul,li,h1,div,video,h1,img,video,#main_1,#main_2,p,footer,header,h2{margin:0px; padding:0px;} 
 ul{list-style:none;}
 a{text-decoration:none; color:#ffffff;}
 h2{text-indent:-9999px; overflow:hidden;}
 body{display:block; font-family:"Daum_Regular"; }
 img{border:none;}

 
@font-face{
font-family:"Daum_Regular";
src:url(Daum_Regular.woff);
src:local(※), url(../font/Daum_Regular.woff) format(‘woff’);
}



 
 
 header{position:absolute; width:100%; height:110px; top:0px; left:0px; background:rgba(255,255,255,0.5); z-index:100; font-family:"Daum_Regular"; }
 header>h1{position:absolute; width:225px; height:60px; left:50%; top:5px; margin-left:-112px; z-index:6;}
 header>nav>ul{position:absolute; width:800px; height:30px; top:75px; left:50%; margin-left:-400px;  color:#472600; z-index:20; } 
 header>nav>ul>li{float:left; width:20%; text-align:center;}
 header>nav>ul>li>a{color:#472600; font-size:14px;}
 header>nav>ul>li>a:hover{border-bottom:1px solid rgba(71,38,0,0.6);}
 
 
 header>nav>ul>li>ul{width:100%; height:100px; margin-top:5px;}
 header>nav>ul>li>ul>li{padding-top:15px;}
 header>nav>ul>li>ul>li>a{display:none; color:#472600; padding-bottom:3px; font-size:12px;}
 header>nav>ul>li>ul>li>a:hover{border-bottom:1px solid rgba(71,38,0,0.6);}
 header>.nav_bg{display:none;position:absolute; top:110px; left:0px; width:100%; height:130px;background:rgba(255,255,255,0.5); }

.embed-container{position:relative; padding-bottom:56.25%; height:0; overflow: hidden; width:100%; margin:0 auto;}
.embed-container iframe{border:1px solid #ccc}
.embed-container iframe, .embed-container object, .embed-container embed {position: absolute; top: 0; left: 0; width:100%; height: 100%;}

 
#main_1{width:100%; margin:0px; padding:0px; overflow:hidden; position:relative; top:0px; left:0px;}
 





 
