  $(function(){
  
  $('header>nav>ul>li').on('mouseenter',function(){
  $('header>.nav_bg, header>nav>ul>li>ul>li>a').stop().fadeIn();
  
  });
  $('header>nav>ul>li,header>.nav_bg').on('mouseleave',function(){
  $('header>.nav_bg,header>nav>ul>li>ul>li>a').stop().fadeOut();
  });
  
  
  $('#sub2>.sub2_2>ul>li').hover(function(){
  
  $(this).find('img[class*=hover]').show();},function(){
 
  $(this).find('img[class*=hover]').hide();
  });
  
  $('#sub2>.sub2_1>ul>li').hover(function(){
  
  $(this).find('img[class*=hover]').show();},function(){
 
  $(this).find('img[class*=hover]').hide();
  });

    var pageNum=1;
	   var wheelChk=true;
	    $('html,body').on ('mousewheel',function(event){
			event.preventDefault();
			if(wheelChk){
				wheelChk=false;
				if(event.deltaY<0){
						pageNum++;
						if(pageNum>=3){ pageNum=3;}
				}else{
					pageNum--;
					if(pageNum<=0){ pageNum=0;}
				}
			}
			$('html,body').stop().animate({scrollTop:$('.sub2_'+pageNum).offset().top},800,function(){ wheelChk=true; });
		});
    $('#sub2>.sub2_1>h2').delay(200).fadeIn();
   
  
  
  
  });
  </script>
  
