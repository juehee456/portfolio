   section,body,header,li,ul,a,h1,h2,div,img,p{margin:0px; padding:0px;}
   img{border:none;}
   
   #container
   { width:100%; height:4000px;
    }
   #container>h2{padding:150px 0px 0px 250px;}

 ul{list-style:none;}
 a{text-decoration:none; color:#ffffff;}


#container>.sub1_0{
width:100%; height:1000px; overflow:hidden;
background:url(../../images/sub1_bg0.jpg) 100% no-repeat;
background-position:0px -100px;
background-size:100% 1100px;
}

#container>.sub1_0>h2{width:658px; height:289px; margin:15% auto 200px auto; display:none;}


