 footer{ position:absolute; width:100%; height:100px; top:3900px; left:0px; background:rgba(255,255,255,0.5); z-index:5; text-align:center;}
 footer>ul>li{float:left; margin-left:15px; margin-top:20px;}
 footer>ul>li:first-child{margin-left:0px;}
