@charset "utf-8";
@import url(http://fonts.googleapis.com/earlyaccess/nanumgothic.css); 





 
body {font-family:"KoPub Dotum"; font-weight: 300;}
 
 @media all and (max-width:767px) {
/* 브라우저 가로폭이 768px이하일때 적용될 css (스마트폰) */
body { background: #f5f5f5; font-size:15px; /* 스마트폰 폰트사이즈 */}
 

#header{width:100%; height:60px; background:#ffffff; overflow:hidden;}
h1{float:left; width:34%; max-width:200px; min-width:150px;  margin:1% 0 0 1%;}
h1 img{width:100%;}
h1 .p{display:none;}
#header .m_menu{width:40px; float:right;margin:7px 5px 0 0;}
#header .m_menu > img{width:100%;}

.top_nav{display:none;}
.gnb{display:none;}




#visual{width:100%; height:100%; background:#ffffff; overflow:hidden;}
#visual img{width:100%; height:100%;}
#visual .t {display:none;}
#visual .p {display:none;}

.bx-wrapper {
  box-shadow:none !important;
  border:none !important;
  background:none !important;
  margin-bottom:0px !important;
.bx-wrapper .bx-pager, .bx-wrapper .bx-controls-auto{bottom:0px !important;}
}




#content1{width:100%; background:#f5f5f5;}
#content1 dl{width:96%; margin-left:2%;}
#content1 dt{ font-size:30px; padding:15px 0 8px 0;  color:#0e76bc; border-bottom:1px solid #969696; font-weight:normal;}
#content1 dd{color:#969696; font-size:16px; font-family:'Nanum Gothic'; padding:5px 0 0 17px; background:url(../images/circle.png) no-repeat; background-position:4px 9px; background-size:7px;}
#content1 .first{margin-top:8px;}
#content1 span{float:right; }



#content2{clear:both;}
.iconbox {overflow:hidden; width:90%;  margin:30px 5%; border-right:1px solid #858585; border-top:1px solid #858585;  background:#f5f5f5;box-sizing:border-box;}
.iconbox li{float:left; width:33%; height:200px; text-align:center;  position:relative; top:0px; left:0px;
border-left:1px solid #858585; border-bottom:1px solid #858585;  overflow:hidden; box-sizing:border-box;}
.iconbox li:nth-of-type(3n){width:34%;}
.iconbox p{width:100%; color:#199cda; font-size:18px;  font-weight:400; position:absolute; display:block;  top:75%; left:0px;}
.iconbox .box1 > img{width:70%; padding-top:5%;}
.iconbox .box2 > img{width:60%; padding-top:5%;}
.iconbox .box3 > img{width:60%; padding-top:5%;}
.iconbox .box4 > img{width:55%; padding-top:5%;}
.iconbox .box5 > img{width:50%; padding-top:5%;}
.iconbox .box6 > img{width:60%; padding-top:15%;}
.iconbox .box7 > img{width:60%; padding-top:20%;}
.iconbox .box8 > img{width:35%; padding-top:5%;}
.iconbox .box9 > img{width:45%; padding-top:5%;}

#slide{display:none;}
#content3{width:100%; height:480px;}
#content3 li{width:100%; height:120px;}
#content3 img{display:none;}
#content3 p{color:#ffffff; font-size:30px;  font-weight:400; padding:15px 0 0 15px; }
#content3 span{display:block; font-size:23px;}
#content3 .li1{background:url(../images/medical_1.png) right no-repeat #3c3c3c; background-size:75px; background-position:97% 20px;}
#content3 .li2{background:url(../images/placeholder_1.png) right no-repeat #24c8d2; background-size:75px; background-position:97% 20px;}
#content3 .li3{background:url(../images/chart_1.png) right no-repeat #1b9dd5; background-size:75px; background-position:97% 20px;}
#content3 .li4{background:url(../images/monitor_1.png) right no-repeat #76b959; background-size:70px; background-position:97% 20px;}

#footer{width:100%; height:62%;margin-top:12.5%; background-color:#3c3c3c; text-align:center;}
#footer .bottom_nav{width:100%;}
#footer  li{width:42%; height:40px; float:left; padding:3% 0 0 7%;}
#footer .line{display:none;}
#footer a{font-size:17px; color:#ffffff;}
#footer img{width:40%; padding:2%;}
#footer p{font-size:13px; color:#959595; width:90%; margin-left:5%; padding-bottom:3%; }
#footer .bl{color:#00ccff;}
#footer .line{display:none;}
}


@media all and (min-width:768px) and (max-width:1023px) {
