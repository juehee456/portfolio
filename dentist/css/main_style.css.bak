#content1 dd{color:#969696;  font-family:'Nanum Gothic';padding:5px 0 0 17px; background:url(../images/circle.png) no-repeat; background-position:4px 11px; background-size:7px;}
#content1 .first{margin-top:8px;}
#content1 .last{padding-bottom:8px;}
#content1 span{float:right; }


#slide{width:50%;  float:right; overflow:hidden;}
#slide img{width:100%;}



#content2{width:50%;}
.iconbox {overflow:hidden; width:98%; margin-left:1%; border-right:1px solid #858585; border-top:1px solid #858585;  background:#f5f5f5; margin-bottom:15px;}
.iconbox li{float:left; width:25%; box-sizing:border-box; height:110px; text-align:center;  position:relative; top:0px; left:0px;
border-left:1px solid #858585; border-bottom:1px solid #858585;  overflow:hidden;}
.iconbox p{width:100%; color:#199cda; font-size:16px; font-weight:400; position:absolute; display:block;  top:75%; left:0px;}
.iconbox .box1 > img{width:70%; padding-top:5%;}
.iconbox .box2 > img{width:60%; padding-top:5%;}
.iconbox .box3 > img{width:60%; padding-top:5%;}
.iconbox .box4 > img{width:55%; padding-top:5%;}
.iconbox .box5 > img{width:50%; padding-top:5%;}
.iconbox .box6 > img{width:60%; padding-top:15%;}
.iconbox .box7{display:none;}
.iconbox .box8 > img{width:35%; padding-top:5%;}
.iconbox .box9 > img{width:45%; padding-top:5%;}


#content3{width:100%; max-width:1000px; overflow:hidden;}
#content3 li{width:25%;  text-align:center; float:left; position:relative; top:0px; left:0px;}
#content3 li > a > img{width:100%;}
#content3 .icon{width:36%; opacity:0.5; position:absolute; top:45%; left:50%; margin-left:-18%;}
#content3 .icon > img{width:100%;}
#content3 p{color:#ffffff; font-size:23px; font-weight:400; text-align:left; position:absolute; top:15px; left:15px;}
#content3 span{display:block; font-size:18px;}

#content3 .li1{background:url(../images/bottom_box_1.jpg) right no-repeat ; background-size:75px; background-position:97% 20px;}
#content3 .li2{background:url(../images/bottom_box_2.jpg) right no-repeat ; background-size:75px; background-position:97% 20px;}
#content3 .li3{background:url(../images/bottom_box_3.jpg) right no-repeat ; background-size:75px; background-position:97% 20px;}
#content3 .li4{background:url(../images/bottom_box_4.jpg) right no-repeat ; background-size:70px; background-position:97% 20px;}

#footer{width:100%; height:150px; margin-top:5%; background-color:#f5f5f5; border-top:1px solid #959595;}
#footer .bottom_nav{width:100%; max-width:1000px; margin:0 auto;  height:40px; background:#f5f5f5; }
#footer li{float:left;  padding-left:10px; margin-top:10px;}
#footer .line{color:#959595;}
#footer a{font-size:14px; color:#959595;}
#footer img{width:20%;  float:left; margin:15px 0 0 15px;}
#footer p{font-size:14px; color:#959595; width:60%; float:left; text-align:center; margin:30px 0 0 15px; }
#footer .bl{color:#0b75bb;}
#footer .bottom_footer{width:100%; height:110px; background:#3c3c3c;}
#footer .footer_wrap{width:1000px; margin:0 auto;}
 
}
 
 
 
